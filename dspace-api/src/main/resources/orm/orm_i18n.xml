<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings 
  xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm orm_2_0.xsd"
  version="2.0">
	<entity class="it.cilea.core.i18n.model.I18n" metadata-complete="false">
		<table name="I18N" schema="public" />
		<attributes>
			<id name="id">
				<generated-value strategy="SEQUENCE" generator="public.I18N_SEQ" />
				<sequence-generator name="public.I18N_SEQ" sequence-name="public.I18N_SEQ" allocation-size="1"/>
			</id>
			<element-collection name="discriminatorSet" fetch="LAZY">
				<column name="DISCRIMINATOR"/>
				<collection-table name="I18N_DATA" schema="public">
					<join-column name="FK_I18N" insertable="true" updatable="true" referenced-column-name="ID"/>
				</collection-table>
			</element-collection>
			<element-collection name="stringMap" fetch="LAZY">
				<map-key-column name="DISCRIMINATOR"/>
				<column name="STRING_VALUE"/>
				<collection-table name="I18N_DATA" schema="public">
					<join-column name="FK_I18N" insertable="true" updatable="true" referenced-column-name="ID"/>
				</collection-table>
			</element-collection>
		</attributes>
	</entity>		
	<entity class="it.cilea.core.i18n.model.I18nData" metadata-complete="false">
		<table name="I18N_DATA" schema="public" />
	</entity>	
	
</entity-mappings>